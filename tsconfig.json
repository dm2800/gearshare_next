{
  "compilerOptions": {

    "baseUrl": "./",
    "paths": {
      "@/components/*": ["./components/*"], 
      "@/app/*": ["/app/*"],
      "@/models/*": ["/models/*"],
      "@/utils/*": ["/utils/*"],
      "@/api/*": ["/api/*"],
    },
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
, "app/chat/[chatId]/page.jsx"  ],
  "exclude": [
    "node_modules"
  ]
}
